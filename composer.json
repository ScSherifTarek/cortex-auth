{
    "name": "cortex/fort",
    "type": "cortex-module",
    "description": "Cortex Fort is a frontend layer for the powerful authentication, authorization and verification package rinvex/fort on top of Laravel. It has all required controllers, views, routes, and other required assets to run a fully functional user management system with complete dashboard out of the box.",
    "keywords": [
        "authentication",
        "authorization",
        "verification",
        "activation",
        "abilities",
        "cortex",
        "phone",
        "sms",
        "call",
        "users",
        "module",
        "security",
        "twofactor",
        "laravel",
        "rinvex",
        "rbac",
        "totp",
        "acl",
        "roles"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-fort/issues",
        "source": "https://github.com/rinvex/cortex-fort",
        "docs": "https://github.com/rinvex/cortex-fort/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-fort/contributors"
        }
    ],
    "require": {
        "php": "^7.0.0",
        "barryvdh/laravel-snappy": "^0.3.0",
        "cortex/foundation": "dev-develop",
        "cortex/fort": "dev-develop",
        "davejamesmiller/laravel-breadcrumbs": "4.x-dev",
        "Illuminate/auth": "~5.5.0",
        "Illuminate/console": "~5.5.0",
        "Illuminate/database": "~5.5.0",
        "Illuminate/http": "~5.5.0",
        "Illuminate/routing": "~5.5.0",
        "Illuminate/support": "~5.5.0",
        "laravel/socialite": "^3.0.0",
        "laravelcollective/html": "^5.4.0",
        "league/fractal": "^0.16.0",
        "nesbot/carbon": "^1.22.0",
        "pragmarx/google2fa": "^2.0.0",
        "proengsoft/laravel-jsvalidation": "dev-master",
        "rinvex/composable": "dev-develop",
        "rinvex/fort": "dev-develop",
        "rinvex/support": "dev-develop",
        "spatie/laravel-activitylog": "^1.15.0",
        "spatie/laravel-menu": "^2.1.0"
    },
    "autoload": {
        "psr-4": {
            "Cortex\\Fort\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Cortex\\Fort\\Providers\\FortServiceProvider"
            ]
        },
        "cortex-module": {
            "autoinstall": false,
            "icon": {
                "color": "#2196f3",
                "name": "heartbeat",
                "background-size": "cover",
                "background-color": "#e3f2fd",
                "background-position": "center",
                "background-repeat": "no-repeat"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
